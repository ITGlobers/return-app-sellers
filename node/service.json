{
  "memory": 512,
  "ttl": 60,
  "timeout": 5000,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "returnRequest": {
      "path": "/_v/return-request/:requestId",
      "public": false,
      "policies": [
        {
          "effect": "allow",
          "actions": ["put"],
          "principals": ["*"]
        }
      ]
    },
    "exportReturns": {
      "path": "/_v/return-request/export",
      "public": true
    }
  }
}
