{
  "vendor": "itglobers",
  "name": "return-app-sellers",
  "version": "0.1.0",
  "title": "Return app seller",
  "description": "Return app seller",
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.format-currency": "0.x",
    "vtex.my-account": "1.x",
    "vtex.my-account-commons": "1.x",
    "vtex.render-runtime": "8.x",
    "vtex.css-handles": "0.x",
    "vtex.easypost": "0.x",
    "vtex.tenant-graphql": "0.x",
    "vtex.catalog-graphql": "1.x"
  },
  "builders": {
    "admin": "0.x",
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*.myvtex.com",
        "path": "/_v/return-request"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/oms/pvt/orders/*"
      }
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "ListOrders"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "sphinx-is-admin"
    },
    {
      "name": "vtex.catalog-graphql:resolve-graphql"
    }
  ],
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "settingsSchema": {
    "title": "Return App Config",
    "type": "object",
    "properties": {
      "parentAccountName": {
        "title": "Parent Account Name",
        "type": "string"
      },
      "appKey": {
        "title": "App Key",
        "type": "string"
      },
      "appToken": {
        "title": "App Token",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
